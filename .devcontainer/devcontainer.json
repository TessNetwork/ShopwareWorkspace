{
    "name": "Shopware",
    "build": {
        "dockerfile": "Dockerfile",
        "args": {
            "SHOPWARE_VERSION": "6.4.18.1"
        }
    },
    "forwardPorts": [8000],
    "remoteUser": "vscode",
    "features": {
        "ghcr.io/devcontainers/features/common-utils:2": {}
	},
    "workspaceMount": "source=${localWorkspaceFolder},target=/shopware/custom/plugins/tessAnalytics,type=bind,consistency=cached",
    "workspaceFolder": "/shopware"
}